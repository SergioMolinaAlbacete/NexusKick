{
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "parcel-bundler": "^1.12.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint"
      ],
      "src/**/*.{js,jsx,json,css,scss}": [
      "prettier --write",
      "git add"
      ]
     
  },
  "name": "nexuskick",
  "description": "TFC FINAL",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel index.html",
    "lint": "eslint --fix src/**/*.{js,jsx}",
    "format": "prettier --write \"*.{js,jsx,json,css}\""
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "semi": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WhiteCleaverPC/NexusKick.git"
  },
